<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.linchong.fastdfsmvtool.dao.TMsgDao">

	<select id="getMsgById" resultType="TMsg" parameterType="Integer">
		select id,title,content,status,type,create_by,create_date,update_date from t_msg where id=#{id};
	</select>


</mapper>
 

